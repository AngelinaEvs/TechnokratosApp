<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.itis.regme">

    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".App"
        android:theme="@style/Theme.RegMe">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presenter.forDelete.RegistrationActivity" />
        <activity android:name=".presenter.forDelete.ProfileActivity" />
        <activity
            android:name=".presenter.forDelete.LoginActivity"
            android:theme="@style/LoginAndRegistration">

        </activity>

        <receiver android:name=".presenter.notifications.NotificationReceiver"/>
    </application>

</manifest>